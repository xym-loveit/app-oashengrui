<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans>
		<form-bean name="form.personal.doc.info" type="org.shengrui.oa.model.admin.ModelDoc"></form-bean>
	</form-beans>
	
	<action-mappings>
		<!-- Home Action -->
		<action path="/app/personal"
				scope="request"
				parameter="action"
				type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="page.my.workplan" path="page.my.workplan" />
			<forward name="page.my.conference" path="page.my.conference" />
			<forward name="page.my.news" path="page.my.news" />
			<forward name="page.my.doc" path="page.my.doc" />
			<forward name="page.my.baron" path="page.my.baron" />
		</action>
		
		<!-- docDwonload-->
		<action path="/app/personal/doc"
				scope="request"
				name="form.personal.doc.info"
				parameter="action"
				type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="page.my.doc" path="page.my.doc" />
			<forward name="page.my.doc.more" path="page.my.doc.more" />
			<forward name="page.my.doc.download" path="page.my.doc.download" />
		</action>
		
	</action-mappings>

</struts-config>
