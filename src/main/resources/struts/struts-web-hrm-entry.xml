<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	
	<action-mappings>
		
		<!-- 员工入职-->
		<action path="/app/hrm/entry"
				scope="request"
				parameter="action"
				type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="hrm.page.entry.index" path="hrm.page.entry.index" />
			<forward name="dialog.page.entry.onboard" path="dialog.page.entry.onboard" />
			<forward name="dialog.page.entry.finalize" path="dialog.page.entry.finalize" />
		</action>
		
	</action-mappings>

</struts-config>
