<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.shengrui.oa.model.ModelSystemOption" table="oa_sysoptions">
        <id name="id" type="string">
            <column name="id" />
            <generator class="native" />
        </id>
        
        <property name="optionName" type="string">
            <column name="option_name" not-null="true" />
        </property>
        
        <property name="optionValue" type="string">
            <column name="option_value" not-null="true" />
        </property>
        
        <property name="optionSlug" type="string">
            <column name="option_slug" not-null="true"/>
        </property>
        
        <property name="optionExtraValue" type="string">
            <column name="option_extra" />
        </property>
        
        <property name="optionVisible" type="java.lang.Integer">
            <column name="option_visible" />
        </property>
        
        <property name="optionCreateTime" type="timestamp">
            <column name="option_ctime" />
        </property>
        
    </class>
</hibernate-mapping>
