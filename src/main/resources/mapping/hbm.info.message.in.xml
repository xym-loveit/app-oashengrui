<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.shengrui.oa.model.info">
    <class name="ModelInMessage" table="app_message_in">
    	<!-- <cache usage="read-write" /> -->
        <id name="id" type="java.lang.String">
            <column name="receive_id" />
            <generator class="native" />
        </id>
        <property name="userId" type="java.lang.Long">
            <column name="user_id"/>
        </property>
        <property name="userFullName" type="java.lang.String">
            <column name="user_name" />
        </property>
        <property name="readFlag" type="java.lang.Integer">
            <column name="flag_read" />
        </property>
        <property name="delFlag" type="java.lang.Integer">
            <column name="flag_del" />
        </property>
        <property name="receiveTime" type="timestamp">
            <column name="receiv_time" />
        </property>
        
        <many-to-one name="shortMessage" class="ModelShortMessage">
        	<column name="msg_id" />
        </many-to-one>
        
	</class>
</hibernate-mapping>
