<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.shengrui.oa.model.hrm">
    <class name="ModelHrmEmployeeApproval" table="app_hrm_employee_approval">
        <id name="id" type="java.lang.Integer">
            <column name="approval_id" />
            <generator class="native" />
        </id>
         <property name="approvalTaskId" type="java.lang.Integer">
            <column name="approval_task_id" />
        </property>
         <property name="approvalComments" type="java.lang.String">
            <column name="apply_comments" />
        </property>
         <property name="entryId" type="java.lang.Integer">
            <column name="entry_id" />
        </property>
         <property name="entryDateTime" type="java.util.Date">
            <column name="entry_datetime" />
        </property>
        
        <many-to-one name="parentId" class="ModelHrmEmployeeApply">
        	<column name="parent_id" />
        </many-to-one>
        
	</class>
</hibernate-mapping>
