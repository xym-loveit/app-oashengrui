<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean name="/app/admin" class="org.shengrui.oa.web.action.admin.AdminAction">
	    <property name="serviceAdminWorkArrange" ref="serviceAdminWorkArrange" />
	    <property name="serviceAdminWorkType" ref="serviceAdminWorkType" />
	</bean>
	
	
	<bean name="/app/admin/doc" class="org.shengrui.oa.web.action.admin.DocManageAction">
	    <property name="serviceDocManage" ref="serviceDocManage" />
	    <property name="serviceDocFile" ref="serviceDocFile" />
	    <property name="serviceDocLevel" ref="serviceDocLevel" />
	    <property name="serviceDocVisiableRange" ref="serviceDocVisiableRange" />
	</bean>
	
	
</beans>