<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	
	<!-- 实例化ActionBean对象，以及传递进去的参数-->
	
	<bean name="/app/system/work/template" class="org.shengrui.oa.web.action.system.sysSettingWorkAction">
		
	</bean>
	
	
	<!-- 工作安排基础设置-->
	<bean name="/app/system/work/base" class="org.shengrui.oa.web.action.system.sysSettingWorkAction">
		<property name="serviceSchoolDistrict" ref="serviceSchoolDistrict" />
	</bean>
	
</beans>