<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	
	<bean id="daoFileAttach" class="cn.trymore.oa.dao.system.impl.DAOFileAttachImpl">
		<property name="hibernateTemplate" ref="hibernateTemplate" />
	</bean>
	
	<bean id="serviceFileAttach" parent="baseTxProxy">
		<property name="target">
			<bean class="cn.trymore.oa.service.system.impl.ServiceFileAttachImpl">
				<constructor-arg index="0" ref="daoFileAttach"/>
			</bean>
		</property>
	</bean>
	
</beans>