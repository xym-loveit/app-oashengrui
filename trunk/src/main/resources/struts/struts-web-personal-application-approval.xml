<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans>
		<form-bean name="form.admin.news" type="org.shengrui.oa.model.news.ModelNewsMag"></form-bean>
		<form-bean name="form.admin.task" type="org.shengrui.oa.model.news.ModelTaskPlan"></form-bean>
		<form-bean name="form.admin.finan" type="org.shengrui.oa.model.news.ModelFinanExpense"></form-bean>
		<form-bean name="form.admin.contract" type="org.shengrui.oa.model.news.ModelFinanContract"></form-bean>
    </form-beans>
	<action-mappings>
		
		<!-- 待我审批 -->
		<action path="/app/personal/approval"
				scope="request"
				parameter="action"
				type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="page.my.approval.index" path="page.my.approval.index" />
			<forward name="page.my.approvalReturn.index" path="page.my.approvalReturn.index" />
		</action>
		
	</action-mappings>

</struts-config>
