<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC 
		"-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" 
		"http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans>
		<form-bean name="form.personal.news.info" type="org.shengrui.oa.model.admin.ModelNewsMag"></form-bean>
	</form-beans>
	
	<action-mappings>
		<!-- Home Action -->
		<action path="/app/personal/news"
				name="form.personal.news.info"
				scope="request"
				parameter="action"
				type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="page.my.news" path="page.my.news" />
		</action>
		
		
	</action-mappings>

</struts-config>
