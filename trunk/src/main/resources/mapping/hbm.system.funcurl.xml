<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.shengrui.oa.model.system">
    <class name="ModelAppFunctionUrl" table="app_function_url">
        <!-- <cache usage="read-write" /> -->
        <id name="id" type="java.lang.String">
            <column name="url_id" />
            <generator class="native" />
        </id>
        <property name="urlPath" type="java.lang.String">
            <column name="url_path" not-null="true" />
        </property>
        
        <many-to-one name="appFunction" class="ModelAppFunction" not-null="false" > 
            <column name="func_id"/> 
        </many-to-one>
         
	</class>
</hibernate-mapping>
