<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.shengrui.oa.model.system">
    <class name="ModelSchoolPositionSet" table="app_school_posets">
        <id name="id" type="java.lang.String">
            <column name="poset_id" />
            <generator class="native" />
        </id>
        <property name="posetName" type="java.lang.String" unique="true">
            <column name="poset_name" not-null="true" />
        </property>
        
        <set name="positions" table="app_school_posets_terms" 
        	lazy="true" inverse="true" cascade="all-delete-orphan" sort="unsorted"> 
	      <key column="poset_id"/>
	      <one-to-many class="ModelSchoolDepartmentPosition"/> 
	    </set>
	    
	</class>
</hibernate-mapping>
