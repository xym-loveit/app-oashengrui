<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.shengrui.oa.model.hrm">
    <class name="ModelHrmEmployee" table="app_hrm_employee ">
        <id name="id" type="java.lang.String">
            <column name="emp_id" />
            <generator class="native" />
        </id>
        <property name="empNo" type="java.lang.String">
            <column name="emp_no" />
        </property>
         <property name="empName" type="java.lang.String">
            <column name="emp_name" />
        </property>
         <property name="birthdate" type="java.util.Date">
            <column name="birthdate" />
        </property>
         <property name="phoneNo" type="java.lang.String">
            <column name="phone_no" />
        </property>
         <property name="shortNo" type="java.lang.String">
            <column name="short_no" />
        </property>
         <property name="positiveDueDate" type="java.util.Date">
            <column name="positive_due_date" />
        </property>
         <property name="contractEndDate" type="java.util.Date">
            <column name="contract_end_date" />
        </property>
         <property name="onboardStatus" type="java.lang.Integer">
            <column name="onboard_status" />
        </property>
         <property name="teacherStar" type="java.lang.Integer">
            <column name="teacher_star" />
        </property>
         <property name="consultStar" type="java.lang.Integer">
            <column name="consult_star" />
        </property>
         <property name="teachClass" type="java.lang.String">
            <column name="teach_class" />
        </property>
         <property name="teachSubject" type="java.lang.String">
            <column name="teach_subject" />
        </property>
         <property name="status" type="java.lang.String">
            <column name="status" />
        </property>
         <property name="entryId" type="java.lang.Integer">
            <column name="entry_id" />
        </property>
         <property name="entryDateTime" type="java.util.Date">
            <column name="entry_datetime" />
        </property>
        <property name="onboardDate" type="java.util.Date">
            <column name="onboard_date" />
        </property>
        
        <many-to-one name="resume" unique="true" class="ModelHrmResume" not-found="ignore">
        	<column name="resume_id" />
        </many-to-one>
        
        <many-to-one name="employeeDistrict" class="org.shengrui.oa.model.system.ModelSchoolDistrict" not-found="ignore">
        	<column name="district_id" />
        </many-to-one>
        
        <many-to-one name="employeeDepartment" class="org.shengrui.oa.model.system.ModelSchoolDepartment" not-found="ignore">
        	<column name="dep_id" />
        </many-to-one>
        
        <many-to-one name="employeePosition" class="org.shengrui.oa.model.system.ModelSchoolDepartmentPosition" not-found="ignore">
        	<column name="position_id" />
        </many-to-one>
        
         <set name="roadMaps" table="app_hrm_employee_roadmap" 
        	cascade="save-update" inverse="false" lazy="true"> 
	      <key column="emp_id"/>  
	      <one-to-many class="ModelHrmEmployeeRoadMap" />
	    </set>
	    
	</class>
</hibernate-mapping>
