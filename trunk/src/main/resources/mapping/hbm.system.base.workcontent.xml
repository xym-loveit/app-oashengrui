<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.shengrui.oa.model.system.ModelBaseWorkContent" table="app_system_work_content">
        <cache usage="read-write" />
        <id name="id" type="java.lang.String">
            <column name="cnt_id" />
            <generator class="native" />
        </id>
         <property name="itemName" type="java.lang.String">
            <column name="item_name" />
        </property>
         <property name="itemValue" type="java.lang.String">
            <column name="item_value" />
        </property>
         <property name="meto" type="java.lang.String">
            <column name="meto" />
        </property>
         <property name="updateTime" type="timestamp">
            <column name="update_time" />
        </property>
        
        <many-to-one name="baseWorkDistrict" class="org.shengrui.oa.model.system.ModelSchoolDistrict">
         <column name="district_id" />
        </many-to-one>
        
   </class>
</hibernate-mapping>
