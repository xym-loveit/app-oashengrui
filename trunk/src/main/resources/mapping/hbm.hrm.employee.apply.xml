<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.shengrui.oa.model.hrm">
    <class name="ModelHrmEmployeeApply" table="app_hrm_employee_apply">
        <id name="id" type="java.lang.Integer">
            <column name="apply_id" />
            <generator class="native" />
        </id>
         <property name="applyForm" type="java.lang.Integer">
            <column name="apply_form_id" />
        </property>
         <property name="applyDate" type="timestamp">
            <column name="apply_date" />
        </property>
         <property name="applyComments" type="java.lang.String">
            <column name="apply_comments" />
        </property>
         <property name="status" type="java.lang.Integer">
            <column name="status" />
        </property>
         <property name="entryId" type="java.lang.Integer">
            <column name="entry_id" />
        </property>
         <property name="entryDateTime" type="timestamp">
            <column name="entry_datetime" />
        </property>
        
        <many-to-one name="employee" class="ModelHrmEmployee">
        	<column name="emp_id" />
        </many-to-one>
        
        <many-to-one name="applyDistrict" class="org.shengrui.oa.model.system.ModelSchoolDistrict" not-found="ignore">
        	<column name="apply_district_id" />
        </many-to-one>
        
        <many-to-one name="applyDepartment" class="org.shengrui.oa.model.system.ModelSchoolDepartment" not-found="ignore">
        	<column name="apply_dep_id" />
        </many-to-one>
        
        <many-to-one name="applyPosition" class="org.shengrui.oa.model.system.ModelSchoolDepartmentPosition" not-found="ignore">
        	<column name="apply_position_id" />
        </many-to-one>
        
	</class>
</hibernate-mapping>
