<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.shengrui.oa.model.flow">
    <class name="ModelProcessDefinition" table="app_process_definition">
        <id name="id" type="java.lang.String">
            <column name="def_id" />
            <generator class="native" />
        </id>
        <property name="processDefName" type="java.lang.String">
            <column name="def_name" />
        </property>
        <property name="processDefDesc" type="java.lang.String">
            <column name="def_description"/>
        </property>
        <property name="condition" type="java.lang.String">
            <column name="def_trigger" />
        </property>
        <property name="createTime" type="timestamp">
            <column name="createtime" />
        </property>
        
        <many-to-one name="filterPoset" class="org.shengrui.oa.model.system.ModelSchoolPositionSet" unique="true">
            <column name="filter_poset" />
        </many-to-one>
        
        <many-to-one name="processType" class="ModelProcessType">
        	<column name="type_id" />
        </many-to-one>
        
         <set name="processTasks" cascade="delete" lazy="true" order-by="task_id asc">
        	<!-- <cache usage="read-write" /> -->
        	<key column="def_id"/>
        	<one-to-many class="ModelProcessTask"/>
        </set>
        
	</class>
</hibernate-mapping>
