<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.shengrui.oa.model.flow">
    <class name="ModelProcessDefinition" table="app_process_definition">
        <id name="id" type="java.lang.String">
            <column name="def_id" />
            <generator class="native" />
        </id>
        <property name="processDefName" type="java.lang.String">
            <column name="def_name" />
        </property>
        <property name="processDefDesc" type="java.lang.String">
            <column name="def_description"/>
        </property>
        <property name="filterPositionNames" type="java.lang.String">
            <column name="filter_posnames" />
        </property>
        <property name="condition" type="java.lang.String">
            <column name="condition" />
        </property>
        <property name="createTime" type="timestamp">
            <column name="createtime" />
        </property>
        
        <many-to-one name="processType" class="ModelProcessType">
        	<column name="type_id" />
        </many-to-one>
        
	</class>
</hibernate-mapping>
