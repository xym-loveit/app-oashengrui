<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	
	<action-mappings>
		
		<!-- 我的申请 -->
		<action path="/app/personal/application_form"
				name="form.hrm.develop"
				scope="request"
				parameter="action"
				type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="page.my.application.form.index" path="page.my.application.form.index" />
			<forward name="dialog.my.application.form" path="dialog.my.application.form" />
			<forward name="tpl.my.application.form.beregular" path="tpl.my.application.form.beregular" />
			<forward name="tpl.my.application.form.transfer" path="tpl.my.application.form.transfer" />
			<forward name="tpl.my.application.form.promotion" path="tpl.my.application.form.promotion" />
			<forward name="tpl.my.application.form.fairwell" path="tpl.my.application.form.fairwell" />
		</action>
		
	</action-mappings>

</struts-config>
