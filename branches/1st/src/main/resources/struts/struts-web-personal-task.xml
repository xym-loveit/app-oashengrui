<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	
	<form-beans>
		<form-bean name="form.task" type="org.shengrui.oa.model.admin.ModelTaskPlan"></form-bean>
	</form-beans>
	
	<action-mappings>
		
		<action path="/app/personal/task"
				name="form.task"
				scope="request"
				parameter="action"
				type="org.springframework.web.struts.DelegatingActionProxy">
			<forward name="page.task.index" path="page.task.index" />
			<forward name="page.task.launched" path="page.task.launched" />
			<forward name="dialog.task.page" path="dialog.task.page" />
			<forward name="data.task.audit.records" path="data.task.audit.records" />
			<forward name="form.task.apply.page" path="form.task.apply.page" />
			<forward name="form.task.audit.page" path="form.task.audit.page" />
		</action>
		
	</action-mappings>

</struts-config>
