<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	
<!-- 实例化DAO和Service的Bean对象-->

	<bean id="daoAppDict" class="org.shengrui.oa.dao.system.impl.DAOAppDictionaryImpl">
		<property name="hibernateTemplate" ref="hibernateTemplate" />
	</bean>
	
	<bean id="serviceAppDictionary" parent="baseTxProxy">
		<property name="target">
			<bean class="org.shengrui.oa.service.system.impl.ServiceAppDictionaryImpl">
				<constructor-arg index="0" ref="daoAppDict"/>
			</bean>
		</property>
	</bean>
	
	
</beans>